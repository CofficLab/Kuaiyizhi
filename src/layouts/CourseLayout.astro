---
import AppLayout from './AppLayout.astro';
import type { BlogLayoutProps } from '../interface/BlogLayoutProps';

const { title, description, headings = [] } = Astro.props as BlogLayoutProps;
---

<AppLayout
    title={title}
    description={description}
    template="doc"
    frontmatter={{
        title: title,
        description: description,
        template: 'doc',
        draft: true,
    }}
    hasSidebar={true}
    sidebar={[]}
    headings={headings}>
    <slot />
</AppLayout>
