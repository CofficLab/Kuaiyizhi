---
import SigninView from '@/views/SigninView.astro';

// 这里定义了每个语言的首页，并将其作为静态页面预渲染。

export const prerender = false;

export async function getStaticPaths() {
  return [
    {
      params: {
        lang: '/zh-cn',
      },
    },
    {
      params: {
        lang: '/en',
      },
    },
  ];
}

const { lang } = Astro.params;
---

<SigninView lang={lang} />
