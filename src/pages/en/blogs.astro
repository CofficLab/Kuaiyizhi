---
import { getBlogs } from '../../database/docs';
import BlogList from '../../components/BlogList.vue';
import BlogsLayout from '../../layouts/BlogsLayout.astro';
const posts = await getBlogs('en');
const enPosts = posts.filter((post) => post.id.startsWith('en'));
---

<BlogsLayout title="所有文章" description="我的博客" lang="en">
    <BlogList posts={enPosts} lang="en" />
</BlogsLayout>
